#!/usr/bin/env bash
# ^^^ for syntax checking in the editor only

case "$1" in 
	--completion-*)
		. "$MDLC_ORIGIN/myx/myx.distro-source/sh-lib/DistroSourceToolsCompletion.include"
		set +e ; return 1
	;;
	''|--help)
		shift
		( . "$MDLC_ORIGIN/myx/myx.distro-system/sh-lib/HelpConsoleDistro.include" )
		set +e ; return 1
	;;
	*)
		echo "DistroImageConsole: Invalid argument: $1" >&2
		set +e ; return 1
	;;
esac
